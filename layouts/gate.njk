{% extends "base.njk" %}

{% block content %}
<h1>{{ title }}</h1>

<p><strong>Symbol:</strong> {{ symbol }}</p>

<h2>Matrix</h2>
<table>
  {% for row in matrix %}
  <tr>
    {% for cell in row %}
    <td>{{ cell }}</td>
    {% endfor %}
  </tr>
  {% endfor %}
</table>

<h2>Description</h2>
<p>{{ description }}</p>

<div>
  {{ content | safe }}
</div>

<a href="/">Back to home</a>
{% endblock %}
