{% extends "base.njk" %}


{% block content %}
<h2>{{ title }}</h2>

<p><strong>Symbol:</strong> ${{ symbol }}$</p>

<div>
  {{ content | safe }}
</div>

<p>This group is contained in the following groups:</p>
<ul>
  {% for group in groups %}
  <li><a href="/groups/{{ group }}">{{ group }}</a></li>
  {% endfor %}
</ul>

<a href="/">Back to home</a>
{% endblock %}
